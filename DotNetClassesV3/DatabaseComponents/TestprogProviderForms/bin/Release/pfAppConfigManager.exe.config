<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="StaticKeysSection" type="pfAppConfigManager.StaticKeysSection, pfAppConfigManager"  />
  </configSections>

  <StaticKeysSection
      MainFormCaption="pfAppConfigManager Application"
      MinAppThreads="1"
      MaxAppThreads="25"
      RequireLogon="False"
      ValidBooleanValues ="true,false,yes,no,y,n,True,False,Yes,No,Y,N,TRUE,FALSE,YES,NO"
    />


  <appSettings>
    <add key="SaveErrorMessagesToErrorLog" value="true"/>
    <add key="ApplicationLogFileName" value="AppConfigManager.log"/>

  </appSettings>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
